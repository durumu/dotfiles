#!/usr/bin/zsh

alias cd1="cd ~/code/code_chat"
alias evr="nvim ~/.config/nvim/init.vim"
alias ezr="nvim ~/.zshrc && source ~/.zshrc"
alias ga="git add ."
alias gc="git commit"
alias gca="git commit --amend"
alias gch="git checkout"
alias gd1="git diff HEAD"
alias gd="git diff"
alias gp="git pull --rebase"
alias gri="git rebase --interactive"
alias grc="git rebase --continue"
alias gs="git status"
alias ipy="ipython"
alias la="ls -la --color=auto"
alias lint="ruff check . --fix"
alias ls="ls --color=auto"
alias py="python3"
alias raserver="RA_MUX_SERVER=~/.config/coc/extensions/coc-rust-analyzer-data/rust-analyzer ~/bin/ra-multiplex/target/release/ra-multiplex-server"
alias rg="rg --smart-case"
alias vcpkg="~/.repos/vcpkg/vcpkg"
alias venv="source ~/tools/venvs/main/bin/activate"
